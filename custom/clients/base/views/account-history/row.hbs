{{#eq rowModel.link.name "accounts_sales_and_services_1"}}
    {{#with rowModel}}
        <tr class="single">
            <td>
                <span class="list">
                    <div class="ellipsis_inline" data-placement="bottom" title="" data-original-title="{{fieldValue this "complete_date_c"}}">
                        {{fieldValue this "complete_date_c"}}
                    </div>
                </span>
            </td>
            <td>
                <span class="list">
                    <div class="ellipsis_inline" data-placement="bottom" title="" data-original-title="{{fieldValue this "name"}}">
                        <a href="#{{buildRoute model=this action="detail"}}">{{fieldValue this "name"}}</a>
                    </div>
                </span>
            </td>
            <td>
                <span class="list">
                    <div class="ellipsis_inline" data-placement="bottom" title="" data-original-title="">
                        {{fieldValue this "actual_rli_quantity_total"}}
                    </div>
                </span>
            </td>
            <td>
                <span class="list">
                    <div class="ellipsis_inline" data-placement="bottom" title="" data-original-title="">
                        {{fieldValue this "primary_rli_uom"}}
                    </div>
                </span>
            </td>
            <td>
                <span class="list">
                    <div class="ellipsis_inline" data-placement="bottom" title="" data-original-title="{{fieldValue this "rli_total_c"}}">
                        {{#if this.attributes.rli_total_c}}${{/if}}{{fieldValue this "rli_total_c"}}
                    </div>
                </span>
            </td>
        </tr>
    {{/with}}
{{else}}
    {{#with rowModel}}
        <li>
            {{#if ../row_actions}}
                <div class="actions pull-right">
                    {{#each ../../row_actions}}
                        {{field ../../../this model=../this}}
                    {{/each}}
                </div>
            {{/if}}
            <p class="pullLeft">
                <a href="#{{buildRoute model=this action="detail"}}">{{fieldValue this "name"}}</a>
            </p>
        </li>
    {{/with}}
{{/eq}}