<div class="span12 control-group transporter clearfix" style="margin-bottom: 0px;">
    <div class="span12 record-label">
        <div class="span1 record-cell">
            <lable>{{str 'LBL_NUM' module}}</lable>
        </div>

        {{#each def.fields}}
            <div class="span{{span}} record-cell {{css_class}}">
                <lable>{{str label ../module}}</lable>
            </div>
        {{/each}}

    </div>
</div>
<div class="transporter-to-insert"></div>
