{{#each value}}
<div>
    <span class="span6">
        <div class="record-label" style="margin-left: 0px;">
            {{str 'LBL_TRANSPORTER' 'HT_Manifest'}}
        </div>
        <span class="normal index">
            <span class="detail" style="padding-left: 0px;">
                {{name}}
            </span>
        </span>
    </span>
    <span class="span6">
        <div class="record-label">
            {{str 'LBL_TRANSFER_DATE' 'HT_Manifest'}}
        </div>
        <span class="normal index">
            <span class="detail">
                {{formatDate transfer_date ../dateOptions}}
            </span>
        </span>
    </span>
</div>
{{/each}}
