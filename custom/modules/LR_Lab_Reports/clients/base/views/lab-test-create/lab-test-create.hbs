<div class="dashboard">
    <div class="cols row-fluid">
        <ul class="dashlets row-fluid">
            <li class="span-12">
                <ul class="dashlet-row">
                    <li class="row-fluid">
                        <div class="rows well well-invisible">
                            <ul class="dashlet-cell rows row-fluid">
                                <li class="dashlet-container span12">
                                    <div class="thumbnail dashlet" data-type="dashlet">
                                        <div data-dashlet="toolbar">
                                            <div class="dashlet-header">
                                                <h4 data-toggle="dashlet" class="dashlet-title">
                                                    {{str 'LBL_LAB_TESTS_DASHLET_VIEW' }}
                                                </h4>
                                            </div>
                                        </div>
                                        <div data-dashlet="dashlet" class="dashlet-content">
                                            <div class="tags" style="padding-top: 5px;padding-left: 5px;">
                                                {{#each tests}}
                                                    <span class="tag-wrapper" style="background: {{this.color}};">
                                                        <span class="tag-name ellipsis_inline" rel="tooltip" data-placement="bottom" title="" data-original-title="{{this.text}}" style="font-size: 15px;line-height: 20px;">
                                                            <a class="tag-name ellipsis-inline" data-action="tag-search" data-tag="{{this.text}}">
                                                                {{this.text}}
                                                            </a>
                                                        </span>
                                                    </span>
                                                {{/each}}
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </li>
                </ul>
            </li>
        </ul>
    </div>
</div>